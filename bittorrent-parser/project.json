{
  "name": "bittorrent-parser",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "bittorrent-parser/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/bittorrent-parser",
        "tsConfig": "bittorrent-parser/tsconfig.lib.json",
        "packageJson": "bittorrent-parser/package.json",
        "main": "bittorrent-parser/src/index.ts",
        "assets": ["bittorrent-parser/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs bittorrent-parser {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "bittorrent-parser/jest.config.ts"
      }
    }
  },
  "tags": []
}
